{
  "name": "book-a-meal",
  "version": "1.0.0",
  "description": "An application that allows customers to make food orders, helps the food vendor decide on what to eat and aids the caterer in managing the business",
  "main": "app.js",
  "scripts": {
    "prestart": "babel-node ./server/startMessage.js",
    "start": "npm-run-all --parallel dev",
    "start:react": "babel-node tools/srcServer.js",
    "start:full": "nodemon --exec babel-node tools/srcFull.js",
    "dev": "nodemon --exec babel-node ./server/app.js",
    "generate-mock-data": "babel-node ./server/dummyBuildScripts/generateMockData",
    "migrate": "sequelize --env=test db:migrate:undo:all && sequelize --env=test db:migrate",
    "seed": "sequelize --env=test db:seed:all",
    "pretest": "npm-run-all migrate seed",
    "test": "cross-env NODE_ENV=test nyc mocha --reporter progress --require babel-core/register \"server/tests/**/*.test.js\" --exit",
    "test:client": "SECRET=mysecret jest --env=jsdom --coverage --verbose",
    "test:client:watch": "SECRET=mysecret jest --env=jsdom --watch --verbose",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "security-check": "nsp check",
    "lint": "eslint \"server/**/*.js\" app.js --fix",
    "build": "npm-run-all build:server build:client",
    "build:server": "babel server/ -d build --copy-files -s",
    "build:client": "webpack --config webpack.config.prod.js ",
    "start-prod": "npm run build && node ./build/app.js",
    "start-full-prod": "babel-node ./tools/srcFull.js",
    "heroku-postbuild": "npm run build",
    "e2e-setup": "selenium-standalone install  --config=./selenium.config.js",
    "test:e2e": "NODE_ENV=test nightwatch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bekomay26/Book-A-Meal.git"
  },
  "author": "Ogunbadejo Folajimi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bekomay26/Book-A-Meal/issues"
  },
  "homepage": "https://github.com/bekomay26/Book-A-Meal#readme",
  "devDependencies": {
    "axios-mock-adapter": "^1.15.0",
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "css-loader": "^0.28.11",
    "enzyme": "^3.5.0",
    "enzyme-adapter-react-16": "^1.3.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "eventsource-polyfill": "^0.9.6",
    "file-loader": "^1.1.11",
    "hard-source-webpack-plugin": "^0.7.0-alpha.3",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.5.0",
    "mocha": "^5.1.1",
    "nodemon": "^1.17.3",
    "npm-run-all": "^4.1.2",
    "nsp": "^3.2.1",
    "nyc": "^11.7.1",
    "open": "0.0.5",
    "react-router-enzyme-context": "^1.2.0",
    "redux-immutable-state-invariant": "^2.1.0",
    "selenium-standalone": "^6.15.2",
    "sequelize-cli": "^4.0.0",
    "style-loader": "^0.21.0",
    "webpack": "^4.8.3",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.2"
  },
  "dependencies": {
    "antd": "^3.8.0",
    "axios": "^0.18.0",
    "babel-core": "^6.26.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "base64-url": "^2.2.0",
    "bcrypt": "^2.0.1",
    "body-parser": "^1.18.2",
    "chai-http": "^4.0.0",
    "chalk": "^2.4.0",
    "classnames": "^2.2.6",
    "cloudinary": "^1.11.0",
    "compression": "^1.7.3",
    "cross-env": "^5.1.4",
    "dotenv": "^5.0.1",
    "enzyme-to-json": "^3.3.4",
    "express": "^4.16.3",
    "express-validator": "^5.2.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "faker": "^4.1.0",
