    "react-redux-toastr": "^7.4.0",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0",
    "semantic-ui-react": "^0.82.2",
    "sequelize": "^4.37.6",
    "supertest": "^3.0.0",
