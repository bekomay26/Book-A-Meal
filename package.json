    "toastr": "^2.1.4",
    "validator": "^10.7.1"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": "[node_module/**/node_modules]",
    "delay": "2500",
    "env": {
      "NODE_ENV": "development",
      "PORT": 5000
    }
  },
  "engines": {
    "node": "8.11.1",
    "npm": "5.6.0"
  }
}
